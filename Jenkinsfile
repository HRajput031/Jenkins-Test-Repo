pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo "Building from DEV branch..."
            }
        }
        stage('Test') {
            steps {
                echo "Running tests from DEV branch..."
            }
        }
        stage('Deploy') {
            steps {
                echo "Deploying DEV branch..."
            }
        }
    }
}

